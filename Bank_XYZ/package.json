  "license": "MIT",
  "dependencies": {
    "cypress": "^13.15.0",
    "prettier": "^3.3.3"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:ci": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/results,overwrite=false,html=false",
    "report:html": "npx mochawesome-merge cypress/results/*.json -o mochawesome.json && npx marge mochawesome.json --reportDir public --inline"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}